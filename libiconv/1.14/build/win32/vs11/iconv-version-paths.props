<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <VSVer>11</VSVer>
    <GlibEtcInstallRoot>$(SolutionDir)\..\..\vs$(VSVer)\$(Configuration)\$(Platform)</GlibEtcInstallRoot>
    <CopyDir>$(GlibEtcInstallRoot)</CopyDir>
    <PackageVersionString>1.14</PackageVersionString>
    <PackageVersionMajor>1</PackageVersionMajor>
    <PackageVersionMinor>14</PackageVersionMinor>
    <PackageVersionMicro>0</PackageVersionMicro>
    <CopyCharsetConfigH>copy ..\..\..\libcharset\config.h.win32 ..\..\..\libcharset\config.h</CopyCharsetConfigH>
    <CopyIconvConfigH>copy ..\..\..\config.h.win32 ..\..\..\config.h</CopyIconvConfigH>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>iconvversionpathsprops</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>HAVE_CONFIG_H;_CRT_SECURE_NO_WARNINGS;_CRT_NONSTDC_NO_WARNINGS;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <DisableSpecificWarnings>4244;4307;4308;%(DisableSpecificWarnings)</DisableSpecificWarnings>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="VSVer">
      <Value>$(VSVer)</Value>
    </BuildMacro>
    <BuildMacro Include="GlibEtcInstallRoot">
      <Value>$(GlibEtcInstallRoot)</Value>
    </BuildMacro>
    <BuildMacro Include="CopyDir">
      <Value>$(CopyDir)</Value>
    </BuildMacro>
    <BuildMacro Include="PackageVersionString">
      <Value>$(PackageVersionString)</Value>
    </BuildMacro>
    <BuildMacro Include="PackageVersionMajor">
      <Value>$(PackageVersionMajor)</Value>
    </BuildMacro>
    <BuildMacro Include="PackageVersionMinor">
      <Value>$(PackageVersionMinor)</Value>
    </BuildMacro>
    <BuildMacro Include="PackageVersionMicro">
      <Value>$(PackageVersionMicro)</Value>
    </BuildMacro>
    <BuildMacro Include="CopyCharsetConfigH">
      <Value>$(CopyCharsetConfigH)</Value>
    </BuildMacro>
    <BuildMacro Include="CopyIconvConfigH">
      <Value>$(CopyIconvConfigH)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>